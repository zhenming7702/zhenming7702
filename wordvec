def download()
    corpus_url="https://dataset.bj.bcebos.com/word2vec/text8.txt"
    web_request=request.get(corpus_url)
    corpus=web_request.content
    with open(:./text8.txt","wb") as f:
        f.write(corpus)
    f.close()
download()
